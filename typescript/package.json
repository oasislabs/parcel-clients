{
  "name": "@oasislabs/parcel",
  "version": "1.0.0-beta.20",
  "license": "Apache-2.0",
  "author": "Oasis Labs <feedback@oasislabs.com>",
  "main": "lib/bundle.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "exports": {
    "require": "./lib/bundle.cjs",
    "import": "./lib/index.js"
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rm -rf ./lib && esbuild src/index.ts --platform=node --target=node14 --bundle --outfile=lib/bundle.cjs --minify && tsc -b",
    "fmt": "xo --fix && prettier --write {tsconfig,package}.json",
    "lint": "yarn lint:tsclient && yarn lint:examples",
    "lint:tsclient": "xo --ignores ./examples && prettier --check {tsconfig,package}.json",
    "lint:examples": "xo ./examples && tools/update-examples-parcel-version.sh --check",
    "test:unit": "node --experimental-vm-modules node_modules/.bin/jest --projects test/jest/unit/",
    "test:examples": "jest --projects test/examples/",
    "test:integration:nodejs": "node test/pure-nodejs.js",
    "test:integration:jest": "jest --projects test/jest/integration/",
    "test:integration:cy-chrome": "start-test 'parcel serve test/cypress/fixtures/index.html -p 4444' 4444 'cypress run'",
    "test:integration:cy-firefox": "start-test 'parcel serve test/cypress/fixtures/index.html -p 4444' 4444 'cypress run --browser firefox --headless'",
    "test:integration": "yarn test:integration:nodejs && yarn test:integration:jest && yarn test:integration:cy-chrome && yarn test:integration:cy-firefox",
    "coverage": "yarn test:unit --coverage",
    "doc": "typedoc --options docs/typedoc.json",
    "prepublishOnly": "yarn build",
    "normalize-permissions-for-packing": "find README.md package.json lib src -type f -exec chmod 664 {} \\;",
    "prepack": "npm run normalize-permissions-for-packing",
    "consistent-pack": "tools/consistent-pack.sh",
    "pack-and-print-integrity-sha": "npm pack --unsafe-perm --silent --json | jq -r '.[].integrity'"
  },
  "nyc": {
    "report-dir": "./coverage/cypress"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@babel/core": "^7.11.6",
    "@cypress/code-coverage": "^3.8.1",
    "@types/bs58": "^4.0.1",
    "@types/jest": "^26.0.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jsrsasign": "^8.0.5",
    "@types/node": "^14.0.20",
    "@types/node-jose": "^1.1.6",
    "ajv": "^8.6.2",
    "ajv-formats": "^2.1.0",
    "bs58": "^4.0.1",
    "cypress": "7.3.0",
    "esbuild": "^0.12.17",
    "eslint-config-xo-typescript": "^0.43.0",
    "eslint-plugin-cypress": "^2.11.2",
    "jest": "^27.0.6",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.4",
    "nock": "^13.1.1",
    "node-jose": "^2.0.0",
    "openapi-types": "^9.1.0",
    "parcel-bundler": "^1.12.4",
    "start-server-and-test": "^1.13.1",
    "tempy": "^1.0.1",
    "ts-jest": "^27.0.4",
    "typedoc": "^0.21.5",
    "typescript": "^4.3.5",
    "xo": "^0.43.0"
  },
  "dependencies": {
    "@mattiasbuelens/web-streams-adapter": "^0.1.0",
    "@types/readable-stream": "^2.3.9",
    "abort-controller": "^3.0.0",
    "eventemitter3": "^4.0.4",
    "form-data": "^4.0.0",
    "jsrsasign": "^10.3.0",
    "ky": "^0.28.5",
    "ky-universal": "^0.9.1",
    "param-case": "^3.0.3",
    "type-fest": "^1.3.0",
    "web-streams-polyfill": "^3.1.0"
  },
  "browserslist": ">2%",
  "engines": {
    "node": ">=14.13"
  }
}
